
#include<bits/stdc++.h>
using namespace std;
int main(){

    string str1, str2;
    cin >> str1 >> str2;
    int flag = 0;
    for (size_t i = 0; i < str1.length(); ++i) {
        char ch1 = tolower(str1[i]);
        char ch2 = tolower(str2[i]);

        if (ch1 < ch2) {
            flag = -1;

        } else if (ch1 > ch2) {
            flag = 1;
        }
    }
    cout<<flag;

return 0;

}
